{
    "name": "view-github-repository-sourcegraph",
    "displayName": "View GitHub Repository with Sourcegraph",
    "version": "1.1.0",
    "preview": false,
    "description": "Open npm package GitHub repository straight from VScode",
    "publisher": "YuTengjing",
    "author": {
        "name": "YuTengjing",
        "url": "https://github.com/tjx666",
        "email": "ytj2713151713@gmail.com"
    },
    "icon": "images/logo.png",
    "license": "SEE LICENSE IN LICENSE",
    "badges": [
        {
            "url": "https://img.shields.io/badge/License-MIT-brightgreen.svg",
            "description": "License: MIT",
            "href": "https://github.com/tjx666/view-github-repository/blob/master/LICENSE"
        },
        {
            "url": "https://img.shields.io/badge/PRs-welcome-brightgreen.svg",
            "description": "PRs Welcome",
            "href": "https://github.com/tjx666/view-github-repository"
        }
    ],
    "homepage": "https://github.com/tjx666/view-github-repository/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/tjx666/view-github-repository/issues",
        "email": "ytj2713151713@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tjx666/view-github-repository"
    },
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "view github repository",
        "view github",
        "view repository",
        "github",
        "repository",
        "github repository",
        "npm",
        "node",
        "nodejs",
        "open",
        "javascript",
        "module",
        "typescript",
        "document",
        "es6",
        "package",
        "npm package",
        "node package",
        "view package",
        "view npm package",
        "view node package",
        "module",
        "types"
    ],
    "activationEvents": [
        "onCommand:viewGithubRepository.viewActiveEditorRepository",
        "onCommand:viewGithubRepository.viewPackageJSONRepository"
    ],
    "main": "dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "viewGithubRepository.viewActiveEditorRepository",
                "title": "view github repository with sourcegraph"
            },
            {
                "command": "viewGithubRepository.viewPackageJSONRepository",
                "title": "view github repository (package.json) with sourcegraph"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run build",
        "compile": "cross-env NODE_ENV=development ts-node --files -P ./scripts/tsconfig.json ./scripts",
        "watch": "cross-env NODE_ENV=development ts-node --files -P ./scripts/tsconfig.json ./scripts --watch",
        "test-compile": "tsc -p ./test/tsconfig.json",
        "watch-tests": "tsc -w -p ./test/tsconfig.json",
        "build": "cross-env NODE_ENV=production ts-node --files -P ./scripts/tsconfig.json ./scripts",
        "build-analyze": "npm build --analyze",
        "lint": "eslint -c .eslintrc.js --ext .ts ./{scripts,src,test}/**/*.ts",
        "test": "npm compile && npm test-compile && node ./dist/test/runTests.js",
        "prepare": "husky install",
        "unprepare": "husky uninstall",
        "package": "vsce package --no-yarn"
    },
    "lint-staged": {
        "*.ts": [
            "eslint -c .eslintrc.js",
            "format-imports --config .vscode/import-sorter.json"
        ],
        "*.{ts,json,md}": [
            "prettier --write"
        ]
    },
    "dependencies": {
        "axios": "^0.24.0",
        "builtin-modules": "^3.2.0",
        "is-online": "^9.0.1",
        "open": "^8.4.0",
        "package-json": "^7.0.0"
    },
    "devDependencies": {
        "@babel/parser": "^7.16.4",
        "@nuxt/friendly-errors-webpack-plugin": "^2.5.2",
        "@types/color": "^3.0.2",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.11.12",
        "@types/pangu": "^3.3.0",
        "@types/pluralize": "^0.0.29",
        "@types/terser-webpack-plugin": "^5.2.0",
        "@types/vscode": "1.62.0",
        "@types/webpack-bundle-analyzer": "^4.4.1",
        "@types/webpack-merge": "^5.0.0",
        "@types/webpack": "^5.28.0",
        "@types/webpackbar": "^4.0.2",
        "@types/yargs": "^17.0.7",
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "@typescript-eslint/parser": "^5.6.0",
        "@vscode/test-electron": "^1.6.2",
        "clean-webpack-plugin": "^4.0.0",
        "color": "^4.1.0",
        "cross-env": "^7.0.3",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-unicorn": "^39.0.0",
        "eslint": "^8.4.1",
        "format-imports": "^2.4.3",
        "glob": "^7.2.0",
        "husky": "7.0.4",
        "jsonc-parser": "^3.0.0",
        "lebab": "^3.1.1",
        "lint-staged": "^12.1.2",
        "mocha": "^9.1.3",
        "pangu": "^4.0.7",
        "pluralize": "^8.0.0",
        "postcss-less": "^5.0.0",
        "postcss-scss": "^4.0.2",
        "postcss": "^8.4.4",
        "prettier": "^2.5.1",
        "recast": "^0.20.5",
        "terser-webpack-plugin": "^5.2.5",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-merge": "^5.8.0",
        "webpack": "^5.65.0",
        "webpackbar": "^5.0.2",
        "yargs": "^17.3.0"
    }
}
